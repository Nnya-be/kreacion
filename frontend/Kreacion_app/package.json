    "aos": "^2.3.4",
    "card-validator": "^9.0.0",
    "formik": "^2.4.5",
    "jarallax": "^2.1.4",
    "react": "^18.2.0",
    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.29.0",
